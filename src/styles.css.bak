body:not(.home) {
  background: #ffffff;
  color: var(--text);
  font-size: 16px;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

/* Page container + header to match services page look */
body:not(.home) .main-container {
  background: var(--white);
  max-width: 1200px;
  width: 100%;
  margin: 0 auto;
  padding: 20px;
  border-radius: 8px;
  box-shadow: var(--shadow);
}

body:not(.home) .main-header {
  background: var(--white);
  box-shadow: var(--shadow);
  border-radius: 8px;
  padding: 12px 20px;
}

/* Headings and type scale like services page */
body:not(.home) h1 {
  font-size: 36px;
  line-height: 1.08;
  color: var(--text);
}
body:not(.home) h2 {
  font-size: 26px;
  color: var(--text);
}

/* Services hero & section styles (if present on a page) */
body:not(.home) .services-hero {
  background: linear-gradient(180deg, rgba(76,175,80,0.06), rgba(255,255,255,0));
  padding: 36px 16px;
  border-radius: 8px;
}
body:not(.home) .hero-content { max-width: 1000px; margin: 0 auto; text-align: center; }

/* Service cards, newsletter and other shared blocks */
body:not(.home) .service-card,
body:not(.home) .property-card,
body:not(.home) .calc-form,
body:not(.home) .valuation-form {
  background: var(--white);
  padding: 20px;
  border-radius: 8px;
  box-shadow: var(--shadow);
}

/* Newsletter block like services page */
body:not(.home) .newsletter-section {
  background: linear-gradient(135deg, var(--primary), var(--primary-dark));
  color: var(--white);
  padding: 40px 25px;
  border-radius: 8px;
  text-align: center;
}

